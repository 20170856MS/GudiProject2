<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
    <mapper namespace="com.iu.home.studyCafe.cafeDAO">
    	<select id="getList" resultType="cafeDTO">
    		SELECT * FROM STUDYCAFE ORDER BY SCNUM ASC
    	</select>
    	
    	<select id="getDetail" parameterType="cafeDetailDTO" resultType="cafeDetailDTO">
    		SELECT * FROM STUDYCAFEDETAIL WHERE SCNAME = #{scName}
    	</select>
    	
    </mapper>